{
  "routes": [
    {
      "route": "/articles/*",
      "rewrite": "/frontend-website/article.html"
    },
    {
      "route": "/admin/*",
      "rewrite": "/frontend-admin/index.html"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/css/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/js/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/images/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.{png,jpg,jpeg,gif,ico,svg,webp}",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.{woff,woff2,ttf,eot}",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/frontend-website/index.html",
    "exclude": ["/admin/*", "/api/*", "/css/*", "/js/*", "/images/*", "/*.{css,js,png,jpg,jpeg,gif,ico,svg,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ".css": "text/css; charset=utf-8",
    ".js": "application/javascript; charset=utf-8",
    ".json": "application/json; charset=utf-8",
    ".html": "text/html; charset=utf-8",
    ".webp": "image/webp"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=3600",
    "Content-Type": "text/html; charset=utf-8",
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "X-XSS-Protection": "1; mode=block",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Permissions-Policy": "geolocation=(), microphone=(), camera=()"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/frontend-website/index.html",
      "statusCode": 200
    }
  }
}
